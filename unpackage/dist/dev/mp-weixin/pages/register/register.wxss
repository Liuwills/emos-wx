.logo {
  margin-top: 80rpx;
  width: 70%;
  margin-left: auto;
  margin-right: auto;
  display: block;
}
.register-container {
  width: 70%;
  margin-top: 150rpx;
  margin-left: auto;
  margin-right: auto;
}
.register-container .register-code {
  border: solid 1rpx #e0e0e0;
  font-size: 34rpx;
  color: #333;
  height: 80rpx;
  padding: 0 20rpx;
  text-align: center;
  margin-bottom: 30rpx;
  background-color: #f5f5f5;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
}
.register-desc {
  color: #333;
  font-size: 28rpx;
  line-height: 1.5;
  margin-bottom: 80rpx;
}
.register-btn {
  background-color: #3474FF;
  color: #fff;
}
.register-btn:active {
  background-color: #0052FF;
}

